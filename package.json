  "description": "init repo for business project",
  "homepage": "https://anka.ricequant.com/crtstal",
  "main": "dist/index.html",
  "repository": {
    "type": "git",
    "url": "ssh://git@git.ricequant.com:7999/node/crystal.git"
  },
  "scripts": {
    "mock": ". ./scripts/loadenv.sh && ./scripts/updep.sh && MOCK=true parcel watch src/index.html --public-url=$PUBLIC_PATH --no-autoinstall",
    "start": ". ./scripts/loadenv.sh && ./scripts/updep.sh && parcel watch src/index.html --public-url=$PUBLIC_PATH --no-autoinstall",
