    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^7.3.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-deep-hash": "^1.0.2",
